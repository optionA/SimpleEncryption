<AnchorPane opacity="0.9" prefHeight="450.0" prefWidth="600.0" style="-fx-background-color: black; -fx-background-image: url(&quot;Icons/space2.jpeg&quot;); -fx-background-size: 100% 100%;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GroupApp.ChangeEncryptController">
