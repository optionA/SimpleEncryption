<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GroupApp.HomeController">
   <children>
      <AnchorPane prefHeight="450.0" prefWidth="600.0">
         <children>
            <HBox layoutX="10.0" layoutY="88.0" prefHeight="350.0" prefWidth="580.0" spacing="5.0">
               <children>
                  <Button fx:id="goToText" mnemonicParsing="false" onAction="#toText" prefHeight="350.0" prefWidth="190.0" text="Encrypt File" textAlignment="CENTER" wrapText="true">
                     <font>
                        <Font name="Arial" size="22.0" />
                     </font>
                  </Button>
                  <Button fx:id="goToChange" mnemonicParsing="false" onAction="#toChange" prefHeight="350.0" prefWidth="190.0" text="Change Key">
                     <font>
                        <Font name="Arial" size="22.0" />
                     </font>
                  </Button>
                  <Button fx:id="goToDecrypt" mnemonicParsing="false" onAction="#toDecrypt" prefHeight="350.0" prefWidth="190.0" text="Decrypt File">
                     <font>
                        <Font name="Arial" size="22.0" />
                     </font>
                  </Button>
               </children>
            </HBox>
            <Label layoutX="4.0" layoutY="5.0" prefHeight="61.0" prefWidth="580.0" text="Simple Encryption" wrapText="true">
               <font>
                  <Font name="Arial Italic" size="36.0" />
               </font>
            </Label>
            <ImageView fitHeight="81.0" fitWidth="91.0" layoutX="519.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@/Icons/FileLock.png" />
               </image>
            </ImageView>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
